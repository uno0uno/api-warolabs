# Database Configuration
NUXT_PRIVATE_DB_USER=your_database_user
NUXT_PRIVATE_DB_HOST=your_database_host
NUXT_PRIVATE_DB_PASSWORD=your_database_password
NUXT_PRIVATE_DB_PORT=5432
NUXT_PRIVATE_DB_NAME=your_database_name

# JWT Authentication
NUXT_PRIVATE_JWT_SECRET=your_jwt_secret_key_here

# Application Settings
NODE_ENV=production
DEBUG=false
FASTAPI_PORT=5001
FASTAPI_HOST=0.0.0.0

# Optional: AWS SES Configuration (if needed)
NUXT_PRIVATE_AWS_ACCES_KEY_ID=your_aws_access_key
NUXT_PRIVATE_AWS_SECRET_ACCESS_KEY=your_aws_secret_key
NUXT_PRIVATE_AWS_REGION=us-west-1
NUXT_PRIVATE_EMAIL_FROM=your_email@domain.com

# Optional: Encryption Keys (if needed)
NUXT_PRIVATE_PRIVATE_KEY_ENCRYPTER=your_private_key
NUXT_PUBLIC_PUBLIC_KEY_ENCRYPTER=your_public_key

# Optional: Better Auth (if needed)
BETTER_AUTH_SECRET_KEY=your_better_auth_secret

# Optional: Base URL
NUXT_PUBLIC_BASE_URL=https://your-domain.com/